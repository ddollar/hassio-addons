{
  "name": "OwnTone",
  "version": "0.0.1",
  "slug": "owntone",
  "description": "OwnTone media server instance",
  "url": "https://github.com/johnpdowling/hassio-addons/owntone",
  "startup": "services",
  "host_network": true,
  "host_dbus": true,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "ports": {
    "3688/tcp": 3688,
    "6600/tcp": 6600,
    "5353/tcp": 5353,
    "3689/tcp": 3689,
    "3690/udp": 3690,
    "3691/udp": 3691
  },
  "ports_description": {
    "3688/tcp": "Websockets",
    "6600/tcp": "MPD",
    "5353/tcp": "Avahi",
    "3689/tcp": "DAAP",
    "3690/udp": "Control port - Airplay",
    "3691/udp": "Timing port - Airplay"
  },
  "map": [
    "config:rw"
  ],
  "options": {
  },
  "schema": {
  }
}
