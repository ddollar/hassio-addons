{
  "name": "OwnTone",
  "version": "0.0.1",
  "slug": "owntone",
  "description": "OwnTone media server instance",
  "url": "https://github.com/johnpdowling/hassio-addons/owntone",
  "startup": "services",
  "host_network": true,
  "host_dbus": true,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "ports": {
    "3688/tcp": 3688,
    "3689/tcp": 3689
  },
  "ports_description": {
    "3688/tcp": "Websockets",
    "3689/tcp": "DAAP"
  },
  "map": [
    "config:rw"
  ],
  "options": {
  },
  "schema": {
  }
}
